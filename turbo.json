{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^db:generate"]
    },
    "format": {},
    "tsc": {
      "dependsOn": ["^db:generate"]
    },
    "test": {
      "dependsOn": ["^db:generate"]
    },
    "test:e2e": {},
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {}
  }
}
